{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "akaunting/laravel-money": "*",
        "akaunting/laravel-setting": "*",
        "arrilot/laravel-widgets": "*",
        "asika/minify": "*",
        "astrotomic/laravel-translatable": "*",
        "bacon/bacon-qr-code": "^2.0",
        "barryvdh/laravel-dompdf": "*",
        "butschster/meta-tags": "^2.3",
        "cyrildewit/eloquent-viewable": "*",
        "ddeboer/tesseract": "1.0",
        "diglactic/laravel-breadcrumbs": "*",
        "doctrine/dbal": "*",
        "google/cloud-vision": "^1.6",
        "google/recaptcha": "^1.3",
        "guzzlehttp/guzzle": "^7.2",
        "haruncpi/laravel-option-framework": "*",
        "igaster/laravel-theme": "*",
        "ilovepdf/ilovepdf-php": "*",
        "io-developer/php-whois": "*",
        "ivopetkov/html5-dom-document-php": "2.*",
        "jackiedo/dotenv-editor": "*",
        "jenssegers/agent": "*",
        "laravel/breeze": "^1.14",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "*",
        "laravel/tinker": "^2.8",
        "league/flysystem-aws-s3-v3": "*",
        "mikemclin/laravel-wp-password": "~2.0.1",
        "mollie/laravel-mollie": "*",
        "nicolaslopezj/searchable": "*",
        "obydul/laraskrill": "*",
        "overtrue/laravel-favorite": "*",
        "pragmarx/google2fa-laravel": "^2.1",
        "razorpay/razorpay": "2.*",
        "seostats/seostats": "dev-master",
        "setasign/fpdi": "^2.3",
        "shetabit/visitor": "*",
        "simplesoftwareio/simple-qrcode": "*",
        "spatie/browsershot": "*",
        "spatie/laravel-image-optimizer": "*",
        "spatie/laravel-medialibrary": "^10.7",
        "spatie/laravel-permission": "*",
        "spatie/laravel-sitemap": "^6.3",
        "spatie/laravel-sluggable": "*",
        "spatie/schema-org": "*",
        "srmklive/paypal": "*",
        "stripe/stripe-php": "*",
        "tecnickcom/tcpdf": "^6.6",
        "thiagoalessio/tesseract_ocr": "*",
        "unicodeveloper/laravel-paystack": "*",
        "watson/active": "*",
        "zanysoft/laravel-zip": "*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "barryvdh/laravel-ide-helper": "^2.12",
        "beyondcode/laravel-query-detector": "^1.6",
        "fakerphp/faker": "^1.9.1",
        "itsgoingd/clockwork": "^5.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "files": [
            "app/Helpers/Helper.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
